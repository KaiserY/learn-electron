{
  "scripts": {
    "start": "npm run compile && electron ./app",
    "postinstall": "install-app-deps",
    "clean:dist": "rimraf dist",
    "clean:public": "rimraf app/public",
    "clean:out": "rimraf app/out",
    "compile:ts": "npm run clean:out && tsc",
    "compile:webpack": "npm run clean:public && webpack --progress --profile --colors --display-reasons --display-error-details --display-modules",
    "compile": "npm run compile:webpack && npm run compile:ts",
    "pack": "npm run compile && build",
    "dist": "npm run compile && build"
  },
  "build": {
    "download": {
      "mirror": "https://npm.taobao.org/mirrors/electron/",
      "strictSSL": false
    },
    "linux": {
      "target": "rpm",
      "depends": []
    },
    "win": {
      "msi": false
    }
  },
  "dependencies": {
    "codemirror": "^5.15.2",
    "electron-sudo": "^3.0.6",
    "jquery": "^3.0.0",
    "material-design-icons": "^2.2.3",
    "material-design-lite": "^1.1.3"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^1.0.0",
    "cpr": "^1.1.2",
    "css-loader": "^0.23.1",
    "devtron": "^1.2.0",
    "electron-builder": "^5.3.0",
    "electron-prebuilt": "^1.2.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.21.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "typescript": "^1.8.10",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-node-externals": "^1.2.0"
  }
}
