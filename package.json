{
  "scripts": {
    "start": "npm run compile && electron ./app",
    "postinstall": "install-app-deps",
    "clean:dist": "rimraf dist",
    "clean:public": "rimraf app/public",
    "clean:out": "rimraf app/out",
    "compile:ts": "npm run clean:out && tsc",
    "compile:webpack": "npm run clean:public && webpack --progress --profile --colors --display-reasons --display-error-details --display-modules",
    "compile": "npm run compile:webpack",
    "pack": "npm run compile && build",
    "dist": "npm run compile && build"
  },
  "build": {
    "download": {
      "mirror": "https://npm.taobao.org/mirrors/electron/",
      "strictSSL": false
    },
    "linux": {
      "target": "rpm",
      "depends": []
    },
    "win": {
      "msi": false
    }
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/forms": "0.1.1",
    "@angular/http": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/router": "3.0.0-alpha.8",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.3",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.9",
    "zone.js": "^0.6.12",
    "codemirror": "^5.16.0",
    "electron-sudo": "^3.0.7",
    "jquery": "^3.0.0",
    "material-design-icons": "^2.2.3",
    "material-design-lite": "^1.1.3"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "copy-webpack-plugin": "^3.0.1",
    "cpr": "^1.1.2",
    "css-loader": "^0.23.1",
    "devtron": "^1.2.1",
    "electron-builder": "^5.10.1",
    "electron-prebuilt": "^1.2.5",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "typescript": "^1.8.10",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-node-externals": "^1.2.0"
  }
}
